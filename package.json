{
  "name": "tok",
  "version": "1.0.0",
  "description": "A Superflare template for Remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:remix": "remix build",
    "dev:remix": "remix watch",
    "dev:superflare": "NODE_ENV=development superflare dev",
    "dev": "npm run build && concurrently \"npm:dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:superflare",
    "build:css": "tailwindcss -i ./global.css -o ./app/tailwind.css",
    "dev:css": "tailwindcss -i ./global.css -o ./app/tailwind.css --watch"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@radix-ui/react-slot": "^1.0.1",
    "@remix-run/cloudflare": "^1.14.1",
    "@remix-run/react": "^1.14.1",
    "@remix-run/serve": "^1.14.1",
    "@superflare/remix": "*",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "isbot": "^3.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superflare": "*",
    "tailwind-merge": "^1.12.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230307.0",
    "@remix-run/dev": "^1.14.1",
    "@remix-run/server-runtime": "^1.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "better-sqlite3": "8.0.1",
    "concurrently": "^7.6.0",
    "or": "^0.2.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "wrangler": "^2.12.2"
  },
  "resolutions": {
    "@cloudflare/workers-types": "4.20230307.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
